<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white">

    <ListView
        android:id="@+id/list_view_dialog"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:divider="@color/colorDivider"
        android:dividerHeight="0dp" />

    <TextView
        android:id="@+id/text_view_cancel"
        android:layout_width="70dp"
        android:layout_height="32dp"
        android:layout_alignBottom="@+id/list_view_dialog"
        android:layout_alignEnd="@+id/list_view_dialog"
        android:gravity="center"
        android:paddingEnd="10dp"
        android:paddingStart="0dp"
        android:paddingTop="12dp"
        android:text="@string/dialog_photo_outlet_cancel_text"
        android:textColor="@color/colorPrimary"
        android:textSize="15sp" />

</RelativeLayout>